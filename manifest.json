{
  "manifest_version": 3,
  "name": "Bomb Party Pooper",
  "version": "1.0.0",
  "description": "Provides word hints while playing Bomb Party",
  "permissions": ["scripting", "activeTab"],
  "host_permissions": ["https://*.jklm.fun/*"],
  "action": {
    "default_popup": "popup/popup.html"
  },
  "web_accessible_resources": [{ "resources": ["words.txt"], "matches": [] }]
}
